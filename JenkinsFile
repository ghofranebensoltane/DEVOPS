pipeline {
    agent any
    stages {
        stage('Checkout Git') {
            steps{
                git branch: 'main', 
                credentialsId: 'Github_Access', 
                url: 'https://github.com/achrefzitoun/Devops.git';
            }
        }
         stage('Compile') {
            steps{
            sh "mvn compile"
            
            }
           
        }
	stage('Build') {
            steps{
            sh "mvn package"
            
            }
	}
          
    }
